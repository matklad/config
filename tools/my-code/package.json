{
    "name": "my-code",
    "displayName": "My Code",
    "description": "My init.el",
    "version": "0.0.0",
    "publisher": "matklad",
    "repository": "https://github.com/matkald/config",
    "license": "MIT OR Apache-2.0",
    "engines": {
        "vscode": "^1.34.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/main.js",
    "contributes": {
        "commands": [
            {
                "command": "my-code.open-notes",
                "title": "Opens a notes file",
                "category": "My Code"
            },
            {
                "command": "my-code.next",
                "title": "Next",
                "category": "My Code"
            },
            {
                "command": "my-code.prev",
                "title": "Prev",
                "category": "My Code"
            },
            {
                "command": "my-code.revealDefinitionAside",
                "title": "Open definition to the side",
                "category": "My Code"
            },
            {
                "command": "inplace-blame.line",
                "title": "Blame line",
                "category": "My Code"
            },
            {
                "command": "inplace-blame.parent",
                "title": "Blame parent",
                "category": "My Code"
            },
            {
                "command": "inplace-blame.undo",
                "title": "Blame undo",
                "category": "My Code"
            }
        ],
        "configuration": [
            {
                "title": "My Code",
                "properties": {
                    "my-code.notes-file": {
                        "type": [
                            "null",
                            "string"
                        ],
                        "default": null,
                        "description": "Notes file to open"
                    }
                }
            }
        ]
    },
    "scripts": {
        "install-extension": "vsce package --allow-star-activation && code --install-extension my-code-0.0.0.vsix",
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./"
    },
    "devDependencies": {
        "@types/node": "^12.12.0",
        "@types/vscode": "^1.34.0",
        "@vscode/vsce": "^3.2.1",
        "typescript": "^4.6.3"
    },
    "extensionPack": [
        "arzg.intellij-theme",
        "arzg.xcode-theme",
        "ban.spellright",
        "denoland.vscode-deno",
        "earshinov.sort-lines-by-selection",
        "Fr43nk.seito-openfile",
        "jakearl.search-editor-apply-changes",
        "jsynowiec.vscode-insertdatestring",
        "jymeng.leuven-vscode",
        "kahole.magit",
        "kaiwood.center-editor-window",
        "maattdd.gitless",
        "matklad.pale-fire",
        "ms-vscode.remote-server",
        "oijaz.unicode-latex",
        "rust-lang.rust-analyzer",
        "ryanabx.djot-vscode",
        "ryu1kn.edit-with-shell",
        "stkb.rewrap",
        "usernamehw.errorlens",
        "usernamehw.find-jump",
        "vadimcn.vscode-lldb",
        "VSpaceCode.whichkey",
        "wmaurer.change-case",
        "wraith13.file-path-bar",
        "yhirose.filtertext",
        "yo1dog.cursor-align",
        "ziglang.vscode-zig"
    ],
    "dependencies": {
    }
}
