{
    "window.enableMenuBarMnemonics": false,
    "window.menuBarVisibility": "hidden",
    "window.restoreWindows": "none",
    "window.title": "${rootName}",
    "window.titleBarStyle": "native",
    "window.zoomLevel": 0,

    "workbench.activityBar.visible": false,
    "workbench.colorTheme": "Pale Fire",
    "workbench.editor.showTabs": false,
    "workbench.panel.defaultLocation": "right",
    "workbench.sideBar.location": "right",
    "workbench.startupEditor": "none",
    "workbench.statusBar.feedback.visible": false,
    "workbench.tree.indent": 16,
    "workbench.useExperimentalGridLayout": true,

    "editor.accessibilitySupport": "off",
    "editor.codeLens": false,
    "editor.cursorBlinking": "solid",
    "editor.cursorStyle": "block",
    "editor.cursorSurroundingLines": 3,
    "editor.dragAndDrop": false,
    "editor.fontFamily": "'JetBrains Mono'",
    "editor.fontLigatures": true,
    "editor.fontSize": 16,
    "editor.formatOnType": true,
    "editor.hover.enabled": false,
    "editor.minimap.enabled": false,
    "editor.parameterHints.enabled": false,
    "editor.renderIndentGuides": false,
    "editor.renderWhitespace": "none",
    "editor.roundedSelection": false,
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false,

    // small gutter
    // "editor.folding": false,
    "editor.glyphMargin": false,
    "editor.lineDecorationsWidth": 0, // undocumented
    "editor.lineNumbers": "off",
    "editor.lineNumbersMinChars": 0, // undocumented

    "breadcrumbs.enabled": true,

    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.openEditors.visible": 0,

    "extensions.ignoreRecommendations": true,
    "extensions.showRecommendationsOnlyOnDemand": true,

    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 100,
    "files.hotExit": "off",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,

    "typescript.suggest.completeFunctionCalls": true,

    "aceJump.finder.pattern": "[ ,-.*{}_()\"'`<>\\/[:&]",
    // "aceJump.placeholder.backgroundColor": "rgb(237, 66, 124)",
    // "aceJump.placeholder.color": "rgb(232, 232, 225)",
    "aceJump.placeholder.backgroundColor": "#3F3F3F",
    "aceJump.placeholder.color": "#BFEBBF",
    "aceJump.placeholder.upperCase": true,
    "aceJump.finder.skipSelection": true,
    "aceJump.scroll.mode": "none",


    "insertDateString.format": "YYYY-MM-DD HH:MM:00 ZZZZ",

    "macros": {
        "closePanelCloseEditorsToggleSidebar": [
            "workbench.action.closePanel",
            "workbench.action.closeEditorsInOtherGroups",
            "workbench.action.closeOtherEditors",
            "workbench.action.toggleSidebarVisibility",
        ],
        "closePanelCloseEditors": [
            "workbench.action.closePanel",
            "workbench.action.closeEditorsInOtherGroups",
            "workbench.action.closeOtherEditors",
        ],
        "focusPanelToggleSidebar": [
            "workbench.action.toggleSidebarVisibility",
            "workbench.action.focusPanel",
        ],
        "closePanelFocusFilesExplorer": [
            "workbench.action.closePanel",
            "workbench.files.action.focusFilesExplorer",
        ]
    },

    "update.enableWindowsBackgroundUpdates": false,
    "update.mode": "none",
    "update.showReleaseNotes": false,

    "liveServer.settings.donotShowInfoMsg": true,

    "rust-analyzer.highlighting.semanticTokens": true,
    "rust-analyzer.highlightingOn": false,
    "rust-analyzer.serverPath": "rust-analyzer",
    "rust-analyzer.featureFlags": {
        "completion.insertion.add-argument-snippets": false,
        "call-info.full": false,
    },

    "asciidoc.preview.useEditorStyle": false,
    "terminal.integrated.fontSize": 12,

    "workbench.colorCustomizations": {
        "editor.background": "#3F3F3F",
        "editor.lineHighlightBackground": "#383838",
        "editorCursor.foreground": "#FFFFEF",
        "editorGutter.background": "#4F4F4F",
        "panel.background": "#3F3F3F",
        "scrollbar.shadow": "#00000000",
        "sideBar.background": "#2B2B2B",
        "statusBar.background": "#2B2B2B",
        "statusBar.foreground": "#8FB28F",
        "statusBar.noFolderBackground": "#2B2B2B",
        "statusBar.noFolderBorder": "#7F9F7F",
        "statusBar.debuggingBackground": "#2B2B2B",
        "statusBar.debuggingBorder": "#DC8CC3",
        "widget.shadow": "#00000000",
    },
    "editor.semanticHighlighting.enabled": true,
    "cSpell.diagnosticLevel": "Hint",
    "cSpell.numSuggestions": 3,
    "cSpell.language": "en,ru",
    "cSpell.userWords": [
        "Cargо",
