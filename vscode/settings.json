{
    "window.enableMenuBarMnemonics": false,
    "window.menuBarVisibility": "hidden",
    "window.restoreWindows": "none",
    "window.title": "${rootName}",
    "window.titleBarStyle": "native",
    "window.zoomLevel": 0,

    "workbench.activityBar.visible": false,
    "workbench.colorTheme": "Zenburn",
    "workbench.editor.showTabs": false,
    "workbench.panel.defaultLocation": "right",
    "workbench.sideBar.location": "right",
    "workbench.startupEditor": "none",
    "workbench.statusBar.feedback.visible": false,
    "workbench.useExperimentalGridLayout": true,

    "editor.codeLens": false,
    "editor.cursorBlinking": "solid",
    "editor.cursorStyle": "block",
    "editor.fontFamily": "'Fira Code'",
    "editor.fontLigatures": true,
    "editor.fontSize": 16,
    "editor.formatOnType": true,
    "editor.hover.enabled": false,
    "editor.minimap.enabled": false,
    "editor.roundedSelection": false,
    "editor.scrollbar.vertical": "hidden",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false,
    // small gutter
    // "editor.folding": false,
    "editor.glyphMargin": false,
    "editor.lineDecorationsWidth": 0, // undocumented
    "editor.lineNumbers": "off",
    "editor.lineNumbersMinChars": 0,  // undocumented
    "editor.renderIndentGuides": false,

    "breadcrumbs.enabled": true,

    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.openEditors.visible": 0,

    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 300,
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "files.hotExit": "off",

    "extensions.ignoreRecommendations": true,
    "extensions.showRecommendationsOnlyOnDemand": true,

    "liveServer.settings.donotShowInfoMsg": true,

    "rust-analyzer.highlightingOn": true,

    "aceJump.finder.pattern": "[ ,-.*{}_()\"'<\\/[:]",
    "aceJump.placeholder.backgroundColor": "2B2B2B",
    "aceJump.placeholder.color": "#8FB28F",
    "aceJump.placeholder.fontFamily": "Ubuntu Mono",
    "aceJump.placeholder.fontWeight": "bold",
    "aceJump.placeholder.upperCase": true,
    "aceJump.finder.skipSelection": true,
    "macros": {
        "closePanelCloseEditorsToggleSidebar": [
            "workbench.action.closePanel",
            "workbench.action.closeEditorsInOtherGroups",
            "workbench.action.closeOtherEditors",
            "workbench.action.toggleSidebarVisibility",
        ],
        "closePanelCloseEditors": [
            "workbench.action.closePanel",
            "workbench.action.closeEditorsInOtherGroups",
            "workbench.action.closeOtherEditors",
        ],
        "focusPanelToggleSidebar": [
            "workbench.action.toggleSidebarVisibility",
            "workbench.action.focusPanel",
        ],
        "closePanelFocusFilesExplorer": [
            "workbench.action.closePanel",
            "workbench.files.action.focusFilesExplorer",
        ]
    },

    "update.enableWindowsBackgroundUpdates": false,
    "update.mode": "none",
    "update.showReleaseNotes": false,

    /*
