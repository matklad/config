{
	"istd": {
		"prefix": "istd",
		"body": [
			"const std = @import(\"std\");"
		],
		"description": "import std"
	},
	"iass": {
		"prefix": "iass",
		"body": [
			"const assert = std.debug.assert;"
		],
		"description": "import assert"
	},
	"psvm": {
		"prefix": "psvm",
		"body": [
			"pub fn main() !void {",
			"    $0",
			"}"
		],
		"description": "main"
	},
	"memu": {
		"prefix": "memu",
		"body": [
			"std.mem.$1(u8, $0)",
		],
		"description": "main"
	},
	"log error": {
		"prefix": "le",
		"body": [
			"log.err(\"{}:$1\", .{$0});"
		],
	},
	"debug print": {
		"prefix": "pd",
		"body": [
			"std.debug.print(\"{}\\n\", .{$0});"
		],
	},
	"tuple": {
		"prefix": "..",
		"body": [
			".{$0}"
		],
	},
	"logthis": {
		"prefix": "logthis",
		"body": [
			"std.testing.log_level = .debug;",
            "defer std.testing.log_level = .err;"
		]
	}
}
