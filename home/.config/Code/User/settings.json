{
    "[html]": {
        "editor.tabSize": 2
    },
    "[markdown]": {
        // "editor.wordWrap": "wordWrapColumn",
        // "editor.wordWrapColumn": 80,
    },
    "projectNameInStatusBar.align": "right",
    "projectNameInStatusBar.textStyle": "uppercase",
    "workbench.panel.opensMaximized": "never",
    "window.enableMenuBarMnemonics": false,
    "window.menuBarVisibility": "hidden",
    "window.newWindowDimensions": "maximized",
    "window.restoreWindows": "none",
    "window.title": "${rootName}",
    "window.titleBarStyle": "native",
    "workbench.editor.showTabs": false,
    "workbench.panel.defaultLocation": "right",
    "workbench.sideBar.location": "right",
    "workbench.startupEditor": "none",
    "workbench.statusBar.feedback.visible": false,
    "workbench.tree.indent": 16,
    "workbench.useExperimentalGridLayout": true,
    "editor.accessibilitySupport": "off",
    "editor.codeLens": false,
    "editor.cursorBlinking": "solid",
    "editor.cursorStyle": "block",
    "editor.cursorSurroundingLines": 5,
    "editor.dragAndDrop": false,
    "editor.fontFamily": "'JetBrains Mono'",
    "editor.fontLigatures": true,
    "editor.fontSize": 16,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.bracketPairColorization.enabled": false,
    "editor.formatOnType": true,
    "editor.hover.enabled": false,
    "editor.minimap.enabled": false,
    "editor.parameterHints.enabled": false,
    "editor.renderWhitespace": "none",
    "editor.roundedSelection": false,
    "editor.semanticHighlighting.enabled": true,
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false,
    "editor.semanticTokenColorCustomizations": {
        "[Xcode 11 Default Light]": {
            "rules": {
                "unresolvedReference": "#FF0050",
            },
        },
        "[Pale Fire High Contrast]": {
            "rules": {
                "unresolvedReference": "#9C6363",
                "parameter": "#DCDCCC",
                "property": "#DFAF8F"
            }
        }
    },
    // small gutter
    // "editor.folding": false,
    "editor.glyphMargin": false,
    "editor.lineDecorationsWidth": 0, // undocumented
    "editor.lineNumbers": "off",
    "editor.lineNumbersMinChars": 0, // undocumented
    "breadcrumbs.enabled": true,
    "breadcrumbs.filePath": "on",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.openEditors.visible": 0,
    "extensions.autoUpdate": true,
    "extensions.ignoreRecommendations": true,
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 100,
    "files.enableTrash": false,
    "files.hotExit": "off",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.sendKeybindingsToShell": false,
    "terminal.integrated.commandsToSkipShell": [
        "language-julia.interrupt",
        "editor.action.marker.nextInFiles"
    ],
    "update.enableWindowsBackgroundUpdates": false,
    "update.mode": "none",
    "update.showReleaseNotes": false,
    "git.alwaysShowStagedChangesResourceGroup": true,
    "debug.onTaskErrors": "debugAnyway",
    "remote.SSH.useLocalServer": false,
    // plugins
    "spellright.suggestionsInHints": false,
    "typescript.suggest.completeFunctionCalls": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "nix.enableLanguageServer": true,
    "nix.serverPath": "nil",
    "insertDateString.format": "YYYY-MM-DD HH:MM:00 ZZZZ",
    "findJump.positionAbsolute": true,
    "errorLens.exclude": [
        ".*Unknown word.*"
    ],
    "whichkey.bindings": [
        {
            "key": "g",
            "name": "go",
            "type": "bindings",
            "bindings": [
                {
                    "key": "d",
                    "name": "definition",
                    "type": "command",
                    "command": "editor.action.revealDefinition",
                },
                {
                    "key": "s",
                    "name": "split definition",
                    "type": "command",
                    "command": "editor.action.revealDefinitionAside"
                },
                {
                    "key": "u",
                    "name": "usage",
                    "type": "command",
                    "command": "references-view.findReferences"
                },
                {
                    "key": "t",
                    "name": "type",
                    "type": "command",
                    "command": "editor.action.goToTypeDefinition"
                },
                {
                    "key": "i",
                    "name": "implementation",
                    "type": "command",
                    "command": "editor.action.goToImplementation"
                },
                {
                    "key": "c",
                    "name": "declarationn",
                    "type": "command",
                    "command": "editor.action.revealDeclaration"
                },
                {
                    "key": "p",
                    "name": "parent",
                    "type": "command",
                    "command": "rust-analyzer.parentModule"
                },
                {
                    "key": "m",
                    "name": "manifest",
                    "type": "command",
                    "command": "rust-analyzer.openCargoToml"
                },
                {
                    "key": "l",
                    "name": "line",
                    "command": "workbench.action.gotoLine"
                },
                {
                    "key": "n",
                    "name": "next...",
                    "type": "bindings",
                    "bindings": [
                        {
                            "key": "c",
                            "name": "change",
                            "type": "command",
                            "command": "workbench.action.editor.nextChange",
                        },
                        {
                            "key": "e",
                            "name": "error",
                            "type": "command",
                            "command": "editor.action.marker.nextInFiles"
                        },
                        {
                            "key": "m",
                            "name": "merge conflict",
                            "type": "command",
                            "command": "merge-conflict.next",
                        },
                    ]
                }
            ]
        },
        {
            "key": "c",
            "name": "cursor",
            "type": "bindings",
            "bindings": [
                {
                    "key": "f",
                    "name": "fold",
                    "type": "command",
                    "command": "editor.fold"
                },
                {
                    "key": "u",
                    "name": "unfold",
                    "type": "command",
                    "command": "editor.unfold"
                },
                {
                    "key": "c",
                    "name": "center",
                    "type": "command",
                    "command": "center-editor-window.center"
                },
                {
                    "key": "s",
                    "name": "start",
                    "type": "command",
                    "command": "cursorTop"
                },
                {
                    "key": "e",
                    "name": "end",
                    "type": "command",
                    "command": "cursorBottom"
                },
            ]
        },
        {
            "key": "s",
            "name": "search",
            "type": "bindings",
            "bindings": [
                {
                    "key": "f",
                    "name": "file",
                    "type": "command",
                    "command": "workbench.action.quickOpen"
                },
                {
                    "key": "s",
                    "name": "string",
                    "type": "command",
                    "command": "actions.find"
                },
                {
                    "key": "g",
                    "name": "grep",
                    "type": "command",
                    "command": "search.action.openNewEditor"
                },
                {
                    "key": "h",
                    "name": "here",
                    "type": "command",
                    "command": "workbench.action.gotoSymbol"
                },
                {
                    "key": "a",
                    "name": "all",
                    "type": "command",
                    "command": "workbench.action.showAllSymbols"
                },
            ]
        },
        {
            "key": "e",
            "name": "edit",
            "type": "bindings",
            "bindings": [
                {
                    "key": "f",
                    "name": "format",
                    "type": "command",
                    "command": "editor.action.formatDocument"
                },
                {
                    "key": "w",
                    "name": "wrap",
                    "type": "command",
                    "command": "rewrap.rewrapComment"
                },
                {
                    "key": "_",
                    "name": "snake_case",
                    "type": "command",
                    "command": "extension.changeCase.snake"
                },
                {
                    "key": "S",
                    "name": "SCREAMING_CASE",
                    "type": "command",
                    "command": "extension.changeCase.constant"
                },
                {
                    "key": "c",
                    "name": "camelCase",
                    "type": "command",
                    "command": "extension.changeCase.camel"
                },
                {
                    "key": "C",
                    "name": "CamelCase",
                    "type": "command",
                    "command": "extension.changeCase.pascal"
                },
                {
                    "key": "j",
                    "name": "join",
                    "type": "command",
                    "command": "editor.action.joinLines"
                },
                {
                    "key": "r",
                    "name": "revert",
                    "type": "command",
                    "command": "git.revertSelectedRanges"
                },
                {
                    "key": "s",
                    "name": "sort",
                    "type": "command",
                    "command": "editor.action.sortLinesAscending"
                },
                {
                    "key": "a",
                    "name": "align",
                    "type": "command",
                    "command": "yo1dog.cursor-align.alignCursors"
                }
            ]
        },
        {
            "key": "o",
            "name": "open",
            "type": "bindings",
            "bindings": [
                {
                    "key": "g",
                    "name": "magit",
                    "type": "command",
                    "command": "runCommands",
                    "args": {
                        "commands": [
                            "workbench.action.closeSidebar",
                            "workbench.action.closePanel",
                            "magit.status"
                        ]
                    }
                },
                {
                    "key": "n",
                    "name": "notes",
                    "type": "command",
                    "command": "my-code.open-notes"
                },
                {
                    "key": "t",
                    "name": "terminal",
                    "type": "command",
                    "command": "terminal.focus",
                },
                {
                    "key": "e",
                    "name": "explorer",
                    "type": "command",
                    "command": "runCommands",
                    "args": {
                        "commands": [
                            "workbench.action.closePanel",
                            "workbench.files.action.focusFilesExplorer"
                        ]
                    }
                },
                {
                    "key": "x",
                    "name": "extensions",
                    "type": "command",
                    "command": "workbench.extensions.action.installExtensions"
                },
                {
                    "key": "s",
                    "name": "settings",
                    "type": "command",
                    "command": "workbench.action.openSettingsJson"
                },
                {
                    "key": "k",
                    "name": "keybindings",
                    "type": "command",
                    "command": "workbench.action.openGlobalKeybindingsFile"
                }
            ]
        },
        {
            "key": "m",
            "name": "misc",
            "type": "bindings",
            "bindings": [
                {
                    "key": "h",
                    "name": "hover",
                    "type": "command",
                    "command": "editor.action.showHover"
                },
                {
                    "key": "a",
                    "name": "argument hints",
                    "type": "command",
                    "command": "editor.action.triggerParameterHints"
                },
            ]
        }
    ],
    "liveServer.settings.donotShowInfoMsg": true,
