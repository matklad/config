{
    "editor.fontSize": 16,
    // "editor.fontSize": 24,
    "terminal.integrated.fontSize": 12,
    "[html]": {
        "editor.tabSize": 2
    },
    "projectNameInStatusBar.align": "right",
    "projectNameInStatusBar.textStyle": "uppercase",
    "workbench.panel.opensMaximized": "never",
    "window.enableMenuBarMnemonics": false,
    "window.menuBarVisibility": "hidden",
    "window.newWindowDimensions": "maximized",
    "window.restoreWindows": "none",
    "window.title": "${rootName}${separator}${activeEditorShort}",
    "window.titleBarStyle": "native",
    "workbench.editor.showTabs": "single",
    "workbench.panel.defaultLocation": "right",
    "workbench.sideBar.location": "right",
    "workbench.startupEditor": "none",
    "workbench.statusBar.feedback.visible": false,
    "workbench.tree.indent": 16,
    "workbench.useExperimentalGridLayout": true,
    "editor.accessibilitySupport": "off",
    "editor.codeLens": false,
    "editor.cursorBlinking": "solid",
    "editor.cursorStyle": "block",
    "editor.cursorSurroundingLines": 5,
    "editor.dragAndDrop": false,
    "editor.fontFamily": "'JetBrains Mono'",
    "editor.fontLigatures": true,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.bracketPairColorization.enabled": false,
    "editor.formatOnType": true,
    "editor.hover.enabled": false,
    "editor.minimap.enabled": false,
    "editor.parameterHints.enabled": false,
    "editor.inlayHints.enabled": "offUnlessPressed",
    "editor.renderWhitespace": "none",
    "editor.roundedSelection": false,
    "editor.semanticHighlighting.enabled": true,
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off",
    "editor.autoIndent": "advanced",
    "editor.semanticTokenColorCustomizations": {
        "[Xcode 11 Default Light]": {
            "rules": {
                "unresolvedReference": "#FF0050",
            },
        },
        "[Pale Fire High Contrast]": {
            "rules": {
                "unresolvedReference": "#9C6363",
                "parameter": "#DCDCCC",
                "property": "#DFAF8F"
            }
        }
    },
    // small gutter
    // "editor.folding": false,
    "editor.glyphMargin": false,
    "editor.lineDecorationsWidth": 0, // undocumented
    "editor.lineNumbers": "off",
    "editor.lineNumbersMinChars": 0, // undocumented
    "breadcrumbs.enabled": true,
    "breadcrumbs.filePath": "on",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.openEditors.visible": 0,
    "extensions.autoUpdate": true,
    "extensions.ignoreRecommendations": true,
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 100,
    "files.enableTrash": false,
    "files.hotExit": "off",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "terminal.integrated.sendKeybindingsToShell": false,
    "terminal.integrated.commandsToSkipShell": [
        "language-julia.interrupt",
        "editor.action.marker.nextInFiles"
    ],
    "update.enableWindowsBackgroundUpdates": false,
    "update.mode": "none",
    "update.showReleaseNotes": false,
    "git.alwaysShowStagedChangesResourceGroup": true,
    "debug.onTaskErrors": "debugAnyway",
    "remote.SSH.useLocalServer": false,
    // plugins
    "spellright.suggestionsInHints": false,
    "typescript.suggest.completeFunctionCalls": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "nix.enableLanguageServer": true,
    "nix.serverPath": "nil",
    "insertDateString.format": "YYYY-MM-DD HH:MM:00 ZZZZ",
    "findJump.positionAbsolute": true,
    "errorLens.exclude": [
        ".*Unknown word.*"
    ],
    "whichkey.bindings": [
        {
            "key": "g",
            "name": "go",
            "type": "bindings",
            "bindings": [
                {
                    "key": "g",
                    "name": "First line",
                    "type": "command",
                    "command": "cursorTop"
                },
                {
                    "key": "l",
                    "name": "Line",
                    "command": "workbench.action.gotoLine"
                },
                {
                    "key": "e",
                    "name": "Last line",
                    "type": "command",
                    "command": "cursorBottom"
                },
                {
                    "key": "d",
                    "name": "Definition",
                    "type": "command",
                    "command": "editor.action.revealDefinition"
                },
                {
                    "key": "D",
                    "name": "Declaration",
                    "type": "command",
                    "command": "editor.action.revealDeclaration"
                },
                {
                    "key": "y",
                    "name": "Type definition",
                    "type": "command",
                    "command": "editor.action.goToTypeDefinition"
                },
                {
                    "key": "r",
                    "name": "References",
                    "type": "command",
                    "command": "references-view.findReferences"
                },
                {
                    "key": "i",
                    "name": "Implementations",
                    "type": "command",
                    "command": "editor.action.goToImplementation"
                },
                {
                    "key": "t",
                    "name": "Top of the window",
                    "type": "command",
                    "command": "cursorMove",
                    "args": {
                        "to": "viewPortTop"
                    }
                },
                {
                    "key": "c",
                    "name": "Center of the window",
                    "type": "command",
                    "command": "cursorMove",
                    "args": {
                        "to": "viewPortCenter"
                    }
                },
                {
                    "key": "b",
                    "name": "Bottom of the window",
                    "type": "command",
                    "command": "cursorMove",
                    "args": {
                        "to": "viewPortBottom"
                    }
                },
                {
                    "key": ".",
                    "name": "Last edited",
                    "type": "command",
                    "command": "workbench.action.navigateToLastEditLocation"
                },
                {
                    "key": "p",
                    "name": "Parent",
                    "type": "command",
                    "command": "rust-analyzer.parentModule"
                },
                {
                    "key": "m",
                    "name": "Manifest",
                    "type": "command",
                    "command": "rust-analyzer.openCargoToml"
                }
            ]
        },
        {
            "key": "e",
            "name": "edit",
            "type": "bindings",
            "bindings": [
                {
                    "key": "f",
                    "name": "Format",
                    "type": "command",
                    "command": "editor.action.formatDocument"
                },
                {
                    "key": "w",
                    "name": "Wrap",
                    "type": "command",
                    "command": "rewrap.rewrapComment"
                },
                {
                    "key": "l",
                    "name": "LaTeX",
                    "type": "command",
                    "command": "unicode-latex.insertMathSymbol"
                },
                {
                    "key": "_",
                    "name": "snake_case",
                    "type": "command",
                    "command": "extension.changeCase.snake"
                },
                {
                    "key": "S",
                    "name": "SCREAMING_CASE",
                    "type": "command",
                    "command": "extension.changeCase.constant"
                },
                {
                    "key": "c",
                    "name": "camelCase",
                    "type": "command",
                    "command": "extension.changeCase.camel"
                },
                {
                    "key": "C",
                    "name": "CamelCase",
                    "type": "command",
                    "command": "extension.changeCase.pascal"
                },
                {
                    "key": "r",
                    "name": "Revert",
                    "type": "command",
                    "command": "git.revertSelectedRanges"
                },
                {
                    "key": "s",
                    "name": "Sort",
                    "type": "command",
                    "command": "editor.action.sortLinesAscending"
                },
                {
                    "key": "a",
                    "name": "Align",
                    "type": "command",
                    "command": "yo1dog.cursor-align.alignCursors"
                }
            ]
        },
        {
            "key": "o",
            "name": "open",
            "type": "bindings",
            "bindings": [
                {
                    "key": "g",
                    "name": "Magit",
                    "type": "command",
                    "command": "runCommands",
                    "args": {
                        "commands": [
                            "workbench.action.closeSidebar",
                            "workbench.action.closePanel",
                            "magit.status"
                        ]
                    }
                },
                {
                    "key": "n",
                    "name": "Notes",
                    "type": "command",
                    "command": "my-code.open-notes"
                },
                {
                    "key": "t",
                    "name": "Terminal",
                    "type": "command",
                    "command": "terminal.focus",
                },
                {
                    "key": "e",
                    "name": "Explorer",
                    "type": "command",
                    "command": "runCommands",
                    "args": {
                        "commands": [
                            "workbench.action.closePanel",
                            "workbench.files.action.focusFilesExplorer"
                        ]
                    }
                },
                {
                    "key": "x",
                    "name": "Extensions",
                    "type": "command",
                    "command": "workbench.extensions.action.installExtensions"
                },
                {
                    "key": "s",
                    "name": "Settings",
                    "type": "command",
                    "command": "workbench.action.openSettingsJson"
                },
                {
                    "key": "S",
                    "name": "Workspace Settings",
                    "type": "command",
                    "command": "workbench.action.openWorkspaceSettingsFile"
                },
                {
                    "key": "k",
                    "name": "Keybindings",
                    "type": "command",
                    "command": "workbench.action.openGlobalKeybindingsFile"
                }
            ]
        },
        {
            "key": "n",
            "name": "next...",
            "type": "bindings",
            "bindings": [
                {
                    "key": "e",
                    "name": "Error",
                    "type": "command",
                    "command": "my-code.next",
                    "args": [
                        "error"
                    ]
                },
                {
                    "key": "c",
                    "name": "Change",
                    "type": "command",
                    "command": "my-code.next",
                    "args": [
                        "change"
                    ]
                },
                {
                    "key": "m",
                    "name": "Merge Conflict",
                    "type": "command",
                    "command": "my-code.next",
                    "args": [
                        "conflict"
                    ]
                },
                {
                    "key": "r",
                    "name": "Reference",
                    "type": "command",
                    "command": "my-code.next",
                    "args": [
                        "reference"
                    ]
                },
                {
                    "key": "f",
                    "name": "Find",
                    "type": "command",
                    "command": "my-code.next",
                    "args": [
                        "occurrence"
                    ]
                }
            ]
        },
        {
            "key": "m",
            "name": "Misc",
            "type": "bindings",
            "bindings": [
                {
                    "key": "h",
                    "name": "Hover",
                    "type": "command",
                    "command": "editor.action.showHover"
                },
                {
                    "key": "a",
                    "name": "Argument Hints",
                    "type": "command",
                    "command": "editor.action.triggerParameterHints"
                },
                {
                    "key": "d",
                    "name": "Diff",
                    "type": "command",
                    "command": "editor.action.dirtydiff.next"
                },
                {
                    "key": "i",
                    "name": "Accept Incoming",
                    "type": "command",
                    "command": "merge-conflict.accept.incoming"
                },
                {
                    "key": "c",
                    "name": "Accept Current",
                    "type": "command",
                    "command": "merge-conflict.accept.current"
                },
                {
                    "key": "b",
                    "name": "Accept Both",
                    "type": "command",
                    "command": "merge-conflict.accept.both"
                },
            ]
        },
    ],
    "liveServer.settings.donotShowInfoMsg": true,
