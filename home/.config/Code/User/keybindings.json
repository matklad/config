[
  { "key": "alt+x",
    "command": "workbench.action.showCommands" },
  { "key": "alt+enter",
    "command": "editor.action.quickFix",
    "when": "editorHasCodeActionsProvider && editorTextFocus && !editorReadonly" },
  { "key": "ctrl+u",
    "command": "rust-analyzer.run" },
  { "key": "ctrl+e g",
    "command": "magit.status" },
  { "key": "ctrl+t",
    "command": "editor.action.formatDocument",
    "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly && !inCompositeEditor" },
  { "key": "ctrl+alt+z",
    "command": "git.revertSelectedRanges" },
  { "key": "ctrl+j",
    "command": "editor.action.joinLines",
    "when": "editorLangId != 'rust'" },
  { "key": "ctrl+j",
    "command": "rust-analyzer.joinLines",
    "when": "editorLangId == 'rust'" },

  { "key": "ctrl+n",
    "command": "editor.action.revealDefinition",
    "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor" },
  { "key": "ctrl+shift+n",
    "command": "editor.action.goToTypeDefinition",
    "when": "editorHasTypeDefinitionProvider && editorTextFocus && !isInEmbeddedEditor" },
  { "key": "ctrl+o",
    "command": "workbench.action.quickOpen" },
  { "key": "ctrl+l",
    "command": "workbench.action.showAllSymbols" },
  { "key": "ctrl+shift+l",
    "command": "workbench.action.gotoSymbol" },
  { "key": "alt+e",
    "command": "extension.aceJump" },
  { "key": "ctrl+i",
    "command": "center-editor-window.center" },
  { "key": "ctrl+e u",
    "command": "rust-analyzer.parentModule" },
  { "key": "ctrl+y",
    "command": "editor.action.marker.nextInFiles",
    "when": "editorFocus && !editorReadonly" },
  { "key": "ctrl+shift+y",
    "command": "editor.action.marker.prevInFiles",
    "when": "editorFocus && !editorReadonly" },
  { "key": "ctrl+down",
    "command": "workbench.action.editor.nextChange",
    "when": "editorTextFocus && !suggestWidgetVisible" },
  { "key": "ctrl+up",
    "command": "workbench.action.editor.previousChange",
    "when": "editorTextFocus && !suggestWidgetVisible" },
  { "key": "ctrl+m",
    "command": "rust-analyzer.matchingBrace" },

  { "key": "ctrl+r",
    "command": "editor.action.rename",
    "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly" },
  { "key": "ctrl+r",
    "command": "renameFile",
    "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus" },

  { "key": "alt+n",
    "command": "editor.action.addSelectionToNextFindMatch" },
  { "key": "ctrl+d",
    "command": "editor.action.smartSelect.expand",
    "when": "editorTextFocus" },
  { "key": "ctrl+shift+d",
    "command": "editor.action.smartSelect.shrink",
    "when": "editorTextFocus" },

  { "key": "ctrl+alt+right",
    "command": "cursorWordPartRight",
    "when": "textInputFocus" },
  { "key": "ctrl+shift+alt+right",
    "command": "cursorWordPartRightSelect",
    "when": "textInputFocus" },
  { "key": "ctrl+alt+left",
    "command": "cursorWordPartStartLeft",
    "when": "textInputFocus" },
  { "key": "ctrl+shift+alt+left",
    "command": "cursorWordPartStartLeftSelect",
    "when": "textInputFocus" },
  { "key": "ctrl+alt+backspace",
    "command": "deleteWordPartLeft",
    "when": "textInputFocus && !editorReadonly" },
  { "key": "ctrl+alt+delete",
    "command": "deleteWordPartRight",
    "when": "textInputFocus && !editorReadonly" },

  { "key": "ctrl+e e",
    "command": "workbench.action.closeWindow",
    "when": "!editorIsOpen && !multipleEditorGroups" },
  { "key": "ctrl+e e",
    "command": "workbench.action.closeGroup",
    "when": "editorFocus && activeEditorGroupEmpty && multipleEditorGroups" },
  { "key": "ctrl+e e",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "sideBarFocus" },
  { "key": "ctrl+e e",
    "command": "workbench.action.closePanel",
    "when": "panelFocus" },
  { "key": "ctrl+e e",
    "command": "workbench.action.closeActiveEditor",
    "when": "!sideBarFocus && !panelFocus" },

  { "key": "ctrl+e 1",
    "command": "macros.closePanelCloseEditorsToggleSidebar",
    "when": "sidebarVisible && editorFocus" },
  { "key": "ctrl+e 1",
    "command": "macros.closePanelCloseEditors",
    "when": "!sidebarVisible && editorFocus" },
  { "key": "ctrl+e 3",
    "command": "workbench.action.splitEditorRight" },
  { "key": "ctrl+e 2",
    "command": "workbench.action.splitEditorDown" },

  { "key": "ctrl+e o",
    "command": "workbench.action.focusNextGroup" },

  { "key": "alt+1",
    "command": "macros.closePanelFocusFilesExplorer",
    "when": "!filesExplorerFocus" },
  { "key": "alt+1",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "filesExplorerFocus" },
  { "key": "alt+2",
    "command": "macros.focusPanelToggleSidebar",
    "when": "!panelFocus && sidebarVisible" },
  { "key": "alt+2",
    "command": "workbench.action.focusPanel",
    "when": "!panelFocus && !sidebarVisible" },
  { "key": "alt+2",
    "command": "workbench.action.togglePanel",
    "when": "panelFocus" },
  { "key": "alt+3",
    "command": "workbench.view.extension.references-view" },

  { "key": "alt+left",
    "command": "workbench.action.navigateBack" },
  { "key": "alt+right",
    "command": "workbench.action.navigateForward" }
]
