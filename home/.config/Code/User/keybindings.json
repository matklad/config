[

// Commands
  { "key": "alt+x",
    "command": "workbench.action.showCommands" },
  { "key": "alt+enter",
    "command": "editor.action.quickFix",
    "when": "editorHasCodeActionsProvider && editorTextFocus && !editorReadonly" },
  { "key": "ctrl+u",
    "command": "rust-analyzer.run",
    "when": "editorLangId == rust" },
  { "key": "ctrl+u",
    "command": "workbench.action.tasks.build",
    "when": "editorLangId != rust" },
  { "key": "ctrl+h",
    "command": "editor.action.formatDocument",
    "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly && !inCompositeEditor" },
  { "key": "ctrl+alt+z",
    "command": "git.revertSelectedRanges" },
  { "key": "ctrl+j",
    "command": "editor.action.joinLines",
    "when": "editorLangId != 'rust'" },
  { "key": "ctrl+j",
    "command": "rust-analyzer.joinLines",
    "when": "editorLangId == 'rust'" },
  { "key": "Enter",
    "command": "rust-analyzer.onEnter",
    "when": "editorTextFocus && !suggestWidgetVisible && editorLangId == rust" },
  { "key": "ctrl+alt+shift+c",
    "command": "workbench.output.action.clearOutput" },
  { "key": "ctrl+shift+f",
    "command": "search.action.openNewEditor",
    "when": "editorTextFocus" },
// Extra Commands
  // { "key": "win+6",
  //   "command": "macros.magit.status" },
  // { "key": "win+7",
  //   "command": "open-notes.open" },

// Window management
  { "key": "ctrl+e",
    "command": "workbench.action.focusNextGroup" },
  { "key": "alt+e",
    "command": "whichkey.show",
    "args": { "narrow": "l" } },

  { "key": "win+k",
    "command": "workbench.action.closeGroup",
    "when": "editorFocus && multipleEditorGroups" },
  { "key": "win+k",
    "command": "workbench.action.closeActiveEditor",
    "when": "inSearchEditor && multipleEditorGroups" },
  { "key": "win+k",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "sideBarFocus" },
  { "key": "win+k",
    "command": "workbench.action.closePanel",
    "when": "panelFocus" },
  { "key": "win+k",
    "command": "workbench.action.closeActiveEditor",
    "when": "!sideBarFocus && !panelFocus && !multipleEditorGroups" },
  { "key": "win+f",
    "command": "macros.closePanelCloseEditorsCloseSidebar",
    "when": "editorFocus" },

  { "key": "win+e",
    "command": "workbench.action.splitEditorRight" },
  { "key": "win+o",
    "command": "workbench.action.splitEditorDown" },
  { "key": "win+n",
    "command": "workbench.action.focusNextGroup" },

  { "key": "win+t",
    "command": "macros.focusFilesExplorerClosePanel",
    "when": "!filesExplorerFocus" },
  { "key": "win+t",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "filesExplorerFocus" },

// Folding
  { "key": "ctrl+,",
    "command": "editor.fold",
    "when": "editorTextFocus && foldingEnabled" },
  { "key": "ctrl+.",
    "command": "editor.unfold",
    "when": "editorTextFocus && foldingEnabled" },

// Semantic Navigation
  { "key": "ctrl+n",
    "command": "editor.action.revealDefinition",
    "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor" },
  {  "key": "alt+n",
     "command": "whichkey.show",
     "args": { "narrow": "s" },
     "when": "editorTextFocus" },
  { "key": "ctrl+o",
    "command": "workbench.action.quickOpen" },
  { "key": "alt+o",
    "command": "whichkey.show",
    "args": { "narrow": "o" } },
  { "key": "ctrl+t",
    "command": "workbench.action.gotoSymbol" },
  { "key": "alt+t",
    "command": "whichkey.show",
    "args": { "narrow": "n" } },

// Text Navigation
  { "key": "ctrl+m",
    "command": "editor.action.jumpToBracket" },
  { "key": "pageup",
    "command": "cursorMove",
    "args": { "by": "wrappedLine", "value": 36, "to": "up" },
    "when": "editorTextFocus" },
  { "key": "shift+pageup",
    "command": "cursorMove",
    "args": { "by": "wrappedLine", "value": 36, "to": "up", "select": true },
    "when": "editorTextFocus" },
  { "key": "pagedown",
    "command": "cursorMove",
    "args": { "by": "wrappedLine", "value": 36, "to": "down" },
    "when": "editorTextFocus" },
  { "key": "shift+pagedown",
    "command": "cursorMove",
    "args": { "by": "wrappedLine", "value": 36, "to": "down", "select": true },
    "when": "editorTextFocus" },
  { "key": "win+0",
    "command": "findJump.activate" },
  { "key": "shift+win+0",
    "command": "findJump.activateWithSelection" },

// Next/Prev
  { "key": "ctrl+down",
    "command": "workbench.action.editor.nextChange",
    "when": "editorTextFocus && !suggestWidgetVisible" },
  { "key": "ctrl+up",
    "command": "workbench.action.editor.previousChange",
    "when": "editorTextFocus && !suggestWidgetVisible" },

  { "key": "alt+down",
    "command": "search.action.focusNextSearchResult",
    "when": "searchViewletVisible" },
  { "key": "alt+up",
    "command": "search.action.focusPreviousSearchResult",
    "when": "searchViewletVisible" },
  { "key": "alt+down",
    "command": "references-view.next",
    "when": "!searchViewletVisible" },
  { "key": "alt+up",
    "command": "references-view.prev",
    "when": "!searchViewletVisible" },
  { "key": "ctrl+y",
    "command": "editor.action.marker.nextInFiles",
    "when": "editorFocus && !editorReadonly" },
  { "key": "ctrl+shift+y",
    "command": "editor.action.marker.prevInFiles",
    "when": "editorFocus && !editorReadonly" },
  { "key": "alt+left",
    "command": "workbench.action.navigateBack" },
  { "key": "alt+right",
    "command": "workbench.action.navigateForward" },

// Misc.

  { "key": "ctrl+c",
    "command": "workbench.action.terminal.copySelection",
    "when": "terminalFocus" },

  { "key": "ctrl+escape",
    "command": "workbench.action.terminal.sendSequence", "args": { "text": "\u0003" },
    "when": "terminalFocus" },

  { "key": "ctrl+i",
    "command": "center-editor-window.center" },

  { "key": "ctrl+r",
    "command": "editor.action.rename",
    "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly" },
  { "key": "ctrl+r",
    "command": "renameFile",
    "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus" },

  { "key": "alt+w",
    "command": "editor.action.addSelectionToNextFindMatch" },
  { "key": "ctrl+w",
    "command": "editor.action.smartSelect.expand",
    "when": "editorTextFocus" },
  { "key": "ctrl+shift+w",
    "command": "editor.action.smartSelect.shrink",
    "when": "editorTextFocus" },

  { "key": "ctrl+alt+right",
    "command": "cursorWordPartRight",
    "when": "textInputFocus" },
  { "key": "ctrl+shift+alt+right",
    "command": "cursorWordPartRightSelect",
    "when": "textInputFocus" },
  { "key": "ctrl+alt+left",
    "command": "cursorWordPartStartLeft",
    "when": "textInputFocus" },
  { "key": "ctrl+shift+alt+left",
    "command": "cursorWordPartStartLeftSelect",
    "when": "textInputFocus" },
  { "key": "ctrl+alt+backspace",
    "command": "deleteWordPartLeft",
    "when": "textInputFocus && !editorReadonly" },
  { "key": "ctrl+alt+delete",
    "command": "deleteWordPartRight",
    "when": "textInputFocus && !editorReadonly" },

// Negatives
  { "key": "down",
    "command": "-selectNextSuggestion",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus" },
  { "key": "up",
    "command": "-selectPrevSuggestion",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus" },
  { "key": "ctrl+n",
    "command": "-workbench.action.files.newUntitledFile" },
  { "key": "ctrl+y",
    "command": "-redo" },
  { "key": "ctrl+[", "command": "-editor.action.outdentLines" },
  { "key": "ctrl+]", "command": "-editor.action.indentLines" },
  { "key": "ctrl+.", "command": "-editor.action.codeAction" },
  { "key": "ctrl+q", "command": "-workbench.action.quit" },
]
