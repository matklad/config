#!/usr/bin/env -S julia --
using Printf

f = tempname()
run(`time --output $f --format '%e %U %S %M' $(ARGS)`)
real, user, sys, rsskb = [parse(Float64, x) for x in split(read(f, String))]

@printf """
real %.2fs
cpu  %.2fs (%.2fs user + %.2fs sys)
rss  %.2fmb
""" real (user + sys) user sys (rsskb / 1024)
