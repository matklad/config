#!/usr/bin/env not-bash

branches = [line.strip() for line in (p> 'git branch --merged').splitlines()]
branches = [
    branch for branch in branches
    if branch != 'master' and not branch.startswith('*')
]

if not branches:
    eprint("no merged branches")
    exit(0)

p> f'git branch -D {" ".join(branches)}'
